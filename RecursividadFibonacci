#include <iostream>

using namespace std;
int fibonacci (int);

int main()
{
    int n, c, i;
    cout<<"Escriba el numero que desea calcular"<<endl;
    cin>>n;
    for (int i=1; c=n; i++)
    cout<<fibonacci(i)<<" "<<endl;
    return 0;
}
int fibonacci (int n)
{
    if (n<=1)
    {
        return n;
    }

else 
{
    return fibonacci(n-1)+fibonacci (n-2);
}
}
